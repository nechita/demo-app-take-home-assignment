{
    "name": "demo-app-dev",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=20.0.0",
        "pnpm": ">=9.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest --",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prepare": "husky",
        "check-types": "tsc --noEmit",
        "prettier": "prettier --write .",
        "pre-commit": "lint-staged && npm run check-types",
        "docker:dev": "pnpm run docker:down && docker-compose --profile dev up --build",
        "docker:prod": "pnpm run docker:down && docker-compose --profile prod up --build",
        "docker:down": "docker-compose down --remove-orphans"
    },
    "dependencies": {
        "@radix-ui/react-avatar": "1.1.2",
        "@radix-ui/react-checkbox": "1.1.3",
        "@radix-ui/react-dialog": "1.1.4",
        "@radix-ui/react-label": "2.1.1",
        "@radix-ui/react-scroll-area": "1.2.2",
        "@radix-ui/react-separator": "1.1.1",
        "@radix-ui/react-slot": "1.1.1",
        "@tanstack/react-query": "^5.63.0",
        "autoprefixer": "^10.4.20",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "framer-motion": "^11.17.0",
        "ioredis": "^5.6.1",
        "lucide-react": "0.469.0",
        "next": "14.1.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "recharts": "^3.0.0",
        "tailwind-merge": "2.6.0",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.2.0",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.1.0",
        "@testing-library/user-event": "14.5.2",
        "@types/jest": "29.5.14",
        "@types/node": "22.0.0",
        "@types/react": "18.2.64",
        "@types/react-dom": "18.2.21",
        "@typescript-eslint/eslint-plugin": "8.19.1",
        "@typescript-eslint/parser": "8.19.1",
        "eslint": "8.57.0",
        "eslint-config-next": "14.1.4",
        "eslint-config-prettier": "^9.1.0",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "15.3.0",
        "pm2": "^6.0.8",
        "postcss": "8.4.49",
        "prettier": "3.4.2",
        "tailwindcss": "3.4.17",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.0",
        "typescript": "5.3.3"
    },
    "packageManager": "pnpm@10.6.5"
}
